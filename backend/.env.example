# OpenAI API配置
OPENAI_API_KEY=your_openai_api_key_here

# 豆包API配置
DOUBAO_API_KEY=your_doubao_api_key_here
DOUBAO_SECRET_KEY=your_doubao_secret_key_here

# 文心一言API配置
ERNIE_ACCESS_TOKEN=your_ernie_access_token_here

# 通义千问API配置
QWEN_API_KEY=your_qwen_api_key_here

# 混元大模型API配置
HUNYUAN_SECRET_ID=your_hunyuan_secret_id_here
HUNYUAN_SECRET_KEY=your_hunyuan_secret_key_here

# Claude API配置
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Gemini API配置
GEMINI_API_KEY=your_gemini_api_key_here

# 服务器配置
HOST=0.0.0.0
PORT=8000
DEBUG=True

# CORS配置
ALLOW_ORIGINS=*

# AI模型配置
AI_MODEL_TYPE=openai  # 可选值：openai, doubao, ernie, qwen, hunyuan, claude, gemini
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=100

# 数据库配置（可选，如需持久化存储）
DATABASE_URL=sqlite:///./app.db
